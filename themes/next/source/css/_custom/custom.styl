body {
    // background:url(/images/saber_test.jpg);
    //background-image:url(https://lewky.cn/images/background/saber1.jpg);
    background-color: #f0f0f4;
    background-repeat: no-repeat;
    background-attachment:fixed;
    background-position:50% 50%;
